<animated-vector xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    tools:targetApi="lollipop">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="284dp"
            android:height="190dp"
            android:viewportWidth="284"
            android:viewportHeight="190">
            <path
                android:name="approve"
                android:pathData="M 5.176 79.96 L 96.564 180.337 L 279.339 5.053"
                android:strokeColor="#FFF"
                android:strokeWidth="20"
                android:trimPathEnd="0"/>
        </vector>
    </aapt:attr>
    <target android:name="approve">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="500"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
